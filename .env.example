# Database Configuration
DATABASE_URL=postgresql+asyncpg://agent_user:agent_pass@postgres:5432/agents_db
DATABASE_SYNC_URL=postgresql://agent_user:agent_pass@postgres:5432/agents_db
POSTGRES_USER=agent_user
POSTGRES_PASSWORD=agent_pass
POSTGRES_DB=agents_db

# Redis Configuration
REDIS_URL=redis://redis:6379
REDIS_HOST=redis
REDIS_PORT=6379

# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=kafka:9092
KAFKA_GROUP_ID=agent-workers

# API Keys
ANTHROPIC_API_KEY=your-anthropic-api-key-here
OPENAI_API_KEY=your-openai-api-key-here

# Service Configuration
ORCHESTRATOR_HOST=0.0.0.0
ORCHESTRATOR_PORT=8000
AGENT_WORKER_HOST=0.0.0.0
AGENT_WORKER_PORT=8001

# Observability
JAEGER_AGENT_HOST=jaeger
JAEGER_AGENT_PORT=6831
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000

# Security
JWT_SECRET_KEY=your-super-secret-jwt-key-change-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=60

# Application Settings
DEBUG=false
LOG_LEVEL=INFO
ENVIRONMENT=development
